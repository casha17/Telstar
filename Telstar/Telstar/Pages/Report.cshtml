@page
@model Telstar.Pages.ReportModel
@if (User.Identity.IsAuthenticated)
{
    <h5 class="card-title">Report Page</h5>
    <div class="row align-items-start spacer">
        <div class="col spacer">
            <Center>
                <ul class="list-group" style="width:300px">
                    <li class="list-group-item summary-listitem"><p class="summary-p">Total Profit:</p><p class="summary-pr" name="profit">0</p></li>
                    <li class="list-group-item summary-listitem"><p class="summary-p">Total Bookings:</p><p class="summary-pr" name="bookings">0</p></li>
                </ul>
            </Center>
            <div class="big-spacer"></div>
        </div>
    </div>
}else
{
    <h3>Not Authenticated</h3>
}





